<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_page">
    <sys_ui_page action="INSERT_OR_UPDATE">
        <category>general</category>
        <client_script/>
        <description>My First Jelly Page</description>
        <direct>false</direct>
        <endpoint>x_58872_needit_jello_world.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
	<g:evaluate>
		var words = ['Hello', 'Jelly', 'World'];
	</g:evaluate>
	<!-- java jexl -->
	<!-- jexl can see js variables 	 -->
	<!-- js makes a copy of jelly variables, jelly -> js -> jxl, jelly -> jxl --> jelly
	<j:forEach var="jvar_word" items="${words}">
		<p> ${jvar_word} </p>
	</j:forEach>
	<g2:evaluate>
		var inc = new GlideRecord('problem');
		inc.query();
	</g2:evaluate>
	<j2:while test="$[inc.next()]">
		<b>$[inc.getValue('number')]</b>
	</j2:while>
</j:jelly>]]></html>
